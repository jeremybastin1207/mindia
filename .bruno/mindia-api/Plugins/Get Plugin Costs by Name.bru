  name: Get Plugin Costs by Name
  type: http
  seq: 7
}

get {
  url: {{baseUrl}}/api/v0/plugins/{{pluginName}}/costs
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

vars:pre-request {
  pluginName: claude_vision
}

docs {
  # Get Plugin Costs by Name
  
  Get detailed usage and cost information for a single plugin.
  
  **Path Parameters**:
  - `pluginName`: plugin identifier (e.g. `claude_vision`)
  
  **Response**:
  - 200 OK: Cost and usage details for the plugin
  - 404 Not Found: Plugin not found
  - 401 Unauthorized: Invalid or missing API key
}

