meta {
  name: Get Plugin Config
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/api/v0/plugins/{{pluginName}}/config
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

vars:pre-request {
  pluginName: claude_vision
}

docs {
  # Get Plugin Config
  
  Retrieve configuration for a plugin.
  
  ## Path Parameters
  - `plugin_name`: Name of the plugin (e.g., `assembly_ai`)
  
  ## Response
  - 200 OK: Plugin configuration
  - 404 Not Found: Plugin configuration not found
  
  Returns plugin configuration including enabled status and plugin-specific settings.
}
